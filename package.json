{
  "name": "@nuintun/bundler",
  "version": "3.0.2",
  "description": "An async file dependency bundle parser.",
  "main": "index.js",
  "typings": "typings/index.d.ts",
  "engines": {
    "node": ">=7.6.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nuintun/bundler.git"
  },
  "keywords": [
    "bundler"
  ],
  "author": {
    "name": "nuintun",
    "email": "nuintun@qq.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nuintun/bundler/issues"
  },
  "scripts": {
    "test": "node tests/bunder.js",
    "prepublishOnly": "npm run build",
    "benchmark": "node tests/benchmark.js",
    "build:main": "rollup -c rollup.config.js",
    "format": "prettier --write src tests index.js typings",
    "build": "npm run build:main && npm run build:typings && npm run format",
    "build:typings": "tsc --declaration --emitDeclarationOnly --declarationDir typings"
  },
  "dependencies": {
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "rimraf": "^3.0.2",
    "rollup": "^2.67.3",
    "prettier": "^2.5.1",
    "typescript": "^4.5.5",
    "@types/node": "^17.0.18",
    "@rollup/plugin-typescript": "^8.3.0"
  },
  "homepage": "https://github.com/nuintun/bundler#readme"
